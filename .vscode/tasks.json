                "-g",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/src/GameStateManager.cpp",
                "${workspaceFolder}/src/GameManager.cpp",
                "${workspaceFolder}/src/Scene.cpp",
