                "${workspaceFolder}/src/TextureManager.cpp",
                "${workspaceFolder}/src/GameObject.cpp",
                "${workspaceFolder}/src/Player.cpp",
                "${workspaceFolder}/src/Enemy.cpp",
                "${workspaceFolder}/src/EnemyState.cpp",
                "${workspaceFolder}/src/enemy_states/EnemyIdleState.cpp",
                "${workspaceFolder}/src/enemy_states/EnemyFollowState.cpp",
                "${workspaceFolder}/src/enemy_states/EnemyAttackState.cpp",
                "${workspaceFolder}/src/Interactable.cpp",
                "${workspaceFolder}/src/interactables/interactable_components/PickableComponent.cpp",
                "${workspaceFolder}/src/interactables/interactable_components/DestroyableComponent.cpp",
                "${workspaceFolder}/src/interactables/interactable_components/InteractableComponent.cpp",
